<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.edgar.oids">

    <uses-permission
        android:required="true"
        android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login"></activity>
        <activity android:name=".ChangePassword"></activity>
        <activity android:name=".Announcements"></activity>
        <activity android:name=".CreateARF"></activity>
        <activity android:name=".AddUser"></activity>
        <activity android:name=".ChangePin"></activity>
        <activity android:name=".Reset"></activity>
        <activity android:name=".CheckArf"></activity>
    </application>

</manifest>
